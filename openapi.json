{
    "openapi": "3.0.1",
    "info": {
      "title": "Library API",
      "version": "v1"
    },
    "servers": [
      {
        "url": "http://localhost:5000",
        "description": "Local server"
      }
    ],
    "paths": {
      "/": {
        "get": {
          "tags": ["Home"],
          "summary": "Home page",
          "responses": {
            "200": {
              "description": "Home page",
              "content": {
                "text/html": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      },
      "/search": {
        "get": {
          "tags": ["Books"],
          "summary": "Search for books by title",
          "parameters": [
            {
              "name": "title",
              "in": "query",
              "required": true,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "limit",
              "in": "query",
              "required": false,
              "schema": {
                "type": "integer"
              }
            },
            {
              "name": "format",
              "in": "query",
              "required": false,
              "schema": {
                "type": "string",
                "enum": ["json", "xml"]
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Books found",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object"
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            },
            "404": {
              "description": "Books not found",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object"
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      },
      "/books": {
        "get": {
          "tags": ["Books"],
          "summary": "Get all books",
          "parameters": [
            {
              "name": "format",
              "in": "query",
              "required": false,
              "schema": {
                "type": "string",
                "enum": ["json", "xml", "protobuf"]
              }
            }
          ],
          "responses": {
            "200": {
              "description": "List of books",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/Book"
                    }
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                },
                "application/x-protobuf": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      },
      "/books/{isbn}/reviews": {
        "post": {
          "tags": ["Reviews"],
          "summary": "Add a review to a book",
          "parameters": [
            {
              "name": "isbn",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "format",
              "in": "query",
              "required": false,
              "schema": {
                "type": "string",
                "enum": ["json", "xml"]
              }
            }
          ],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Review"
                }
              }
            }
          },
          "responses": {
            "201": {
              "description": "Review added",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Review"
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            },
            "404": {
              "description": "Book not found",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object"
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "get": {
          "tags": ["Reviews"],
          "summary": "Get all reviews for a book",
          "parameters": [
            {
              "name": "isbn",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "format",
              "in": "query",
              "required": false,
              "schema": {
                "type": "string",
                "enum": ["json", "xml"]
              }
            }
          ],
          "responses": {
            "200": {
              "description": "List of reviews",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/Review"
                    }
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            },
            "404": {
              "description": "Book not found",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object"
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      },
      "/books/{isbn}/reviews/{id}": {
        "put": {
          "tags": ["Reviews"],
          "summary": "Update a review for a book",
          "parameters": [
            {
              "name": "isbn",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "id",
              "in": "path",
              "required": true,
              "schema": {
                "type": "integer"
              }
            },
            {
              "name": "format",
              "in": "query",
              "required": false,
              "schema": {
                "type": "string",
                "enum": ["json", "xml"]
              }
            }
          ],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Review"
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Review updated",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Review"
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            },
            "404": {
              "description": "Book or review not found",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object"
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "delete": {
          "tags": ["Reviews"],
          "summary": "Delete a review for a book",
          "parameters": [
            {
              "name": "isbn",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "id",
              "in": "path",
              "required": true,
              "schema": {
                "type": "integer"
              }
            },
            {
              "name": "format",
              "in": "query",
              "required": false,
              "schema": {
                "type": "string",
                "enum": ["json", "xml"]
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Review deleted",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Review"
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            },
            "404": {
              "description": "Book or review not found",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object"
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      },
      "/likes": {
        "get": {
          "tags": ["Favorites"],
          "summary": "Get all favorites",
          "parameters": [
            {
              "name": "format",
              "in": "query",
              "required": false,
              "schema": {
                "type": "string",
                "enum": ["json", "xml"]
              }
            }
          ],
          "responses": {
            "200": {
              "description": "List of favorites",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/Favorite"
                    }
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      },
      "/likes/{isbn}": {
        "put": {
          "tags": ["Favorites"],
          "summary": "Add a book to favorites",
          "parameters": [
            {
              "name": "isbn",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "format",
              "in": "query",
              "required": false,
              "schema": {
                "type": "string",
                "enum": ["json", "xml"]
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Book added to favorites",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Favorite"
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            },
            "404": {
              "description": "Book not found",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object"
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "delete": {
          "tags": ["Favorites"],
          "summary": "Remove a book from favorites",
          "parameters": [
            {
              "name": "isbn",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "format",
              "in": "query",
              "required": false,
              "schema": {
                "type": "string",
                "enum": ["json", "xml"]
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Book removed from favorites",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Favorite"
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            },
            "404": {
              "description": "Book not found",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object"
                  }
                },
                "application/xml": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "Book": {
          "type": "object",
          "properties": {
            "isbn": {
              "type": "string"
            },
            "title": {
              "type": "string"
            },
            "author": {
              "type": "string"
            }
          }
        },
        "Review": {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "review": {
              "type": "string"
            },
            "reviewer": {
              "type": "string"
            }
          }
        },
        "Favorite": {
          "type": "object",
          "properties": {
            "isbn": {
              "type": "string"
            },
            "title": {
              "type": "string"
            },
            "author": {
              "type": "string"
            }
          }
        }
      }
    }
  }
  